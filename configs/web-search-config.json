{
  "version": "2.0.0",
  "command": "npx",
  "options": {
    "logLevel": "info",
    "host": "localhost",
    "port": 7802
  },
  "healthCheck": {
    "endpoint": "/health",
    "timeout": 15,
    "interval": 120
  },
  "description": "MCP server for web search via Brave Search API",
  "args": [
    "-y",
    "@modelcontextprotocol/server-brave-search"
  ],
  "env": {
    "BRAVE_API_KEY": "${env:BRAVE_API_KEY}"
  },
  "name": "web-search-server"
}
