{
  "version": "2.0.0",
  "command": "npx",
  "options": {
    "logLevel": "info",
    "host": "localhost",
    "port": 7801
  },
  "healthCheck": {
    "endpoint": "/health",
    "timeout": 15,
    "interval": 120
  },
  "description": "MCP server for GitHub integration",
  "args": [
    "-y",
    "@modelcontextprotocol/server-github"
  ],
  "env": {
    "GITHUB_TOKEN": "${env:GITHUB_TOKEN}"
  },
  "name": "github-server"
}
